	local playersScrollFrame = Instance.new("ScrollingFrame")
	playersScrollFrame.Size = UDim2.new(1, -20, 1, -45)
	playersScrollFrame.Position = UDim2.new(0, 10, 0, 40)
	playersScrollFrame.BackgroundTransparency = 1
	playersScrollFrame.BorderSizePixel = 0
	playersScrollFrame.ScrollBarThickness = 4
	playersScrollFrame.ScrollBarImageColor3 = getThemeColor()
	playersScrollFrame.Parent = playersListFrame

	local playersScrollLayout = Instance.new("UIListLayout")
	playersScrollLayout.Padding = UDim.new(0, 5)
	playersScrollLayout.Parent = playersScrollFrame

	local function updatePlayersList()
		for _, child in pairs(playersScrollFrame:GetChildren()) do
			if child:IsA("Frame") then
				child:Destroy()
			end
		end

		for _, plr in pairs(Players:GetPlayers()) do
			if plr ~= player then
				local playerFrame = Instance.new("Frame")
				playerFrame.Size = UDim2.new(1, -10, 0, 60)
				playerFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
				playerFrame.BorderSizePixel = 0
				playerFrame.Parent = playersScrollFrame

				local playerCorner = Instance.new("UICorner")
				playerCorner.CornerRadius = UDim.new(0, 6)
				playerCorner.Parent = playerFrame

				local playerName = Instance.new("TextLabel")
				playerName.Size = UDim2.new(1, -10, 0, 20)
				playerName.Position = UDim2.new(0, 5, 0, 5)
				playerName.BackgroundTransparency = 1
				playerName.Text = plr.Name
				playerName.TextColor3 = Color3.fromRGB(255, 255, 255)
				playerName.TextSize = 14
				playerName.Font = Enum.Font.GothamBold
				playerName.TextXAlignment = Enum.TextXAlignment.Left
				playerName.Parent = playerFrame

				local playerInfo = Instance.new("TextLabel")
				playerInfo.Size = UDim2.new(1, -10, 0, 15)
				playerInfo.Position = UDim2.new(0, 5, 0, 25)
				playerInfo.BackgroundTransparency = 1
				playerInfo.TextColor3 = Color3.fromRGB(200, 200, 200)
				playerInfo.TextSize = 11
				playerInfo.Font = Enum.Font.Gotham
				playerInfo.TextXAlignment = Enum.TextXAlignment.Left
				playerInfo.Parent = playerFrame

				-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ñ–æ
				local function updateInfo()
					local info = "ID: " .. plr.UserId
					if plr.Character then
						local humanoid = plr.Character:FindFirstChild("Humanoid")
						if humanoid then
							info = info .. " | HP: " .. math.floor(humanoid.Health) .. "/" .. math.floor(humanoid.MaxHealth)
						end
						
						if player.Character and player.Character:FindFirstChild("HumanoidRootPart") and plr.Character:FindFirstChild("HumanoidRootPart") then
							local distance = (player.Character.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude
							info = info .. " | " .. math.floor(distance) .. "m"
						end
					end
					playerInfo.Text = info
				end

				updateInfo()
				
				task.spawn(function()
					while playerFrame.Parent do
						updateInfo()
						task.wait(0.5)
					end
				end)
			end
		end

		playersScrollFrame.CanvasSize = UDim2.new(0, 0, 0, playersScrollLayout.AbsoluteContentSize.Y)
	end

	-- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
	task.spawn(function()
		while true do
			if playersContainer.Visible then
				updatePlayersList()
			end
			task.wait(1)
		end
	end)

	Players.PlayerAdded:Connect(function()
		if playersContainer.Visible then
			updatePlayersList()
		end
	end)

	Players.PlayerRemoving:Connect(function()
		if playersContainer.Visible then
			updatePlayersList()
		end
	end)

	playersContainer.CanvasSize = UDim2.new(0, 0, 0, playersLayout.AbsoluteContentSize.Y + 10)
	playersLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		playersContainer.CanvasSize = UDim2.new(0, 0, 0, playersLayout.AbsoluteContentSize.Y + 10)
	end)

	-- –ö–Ω–æ–ø–∫–∞ –æ—Ç–∫—Ä—ã—Ç–∏—è GUI
	local openButton = Instance.new("TextButton")
	openButton.Size = UDim2.new(0, 150, 0, 45)
	openButton.Position = UDim2.new(0.5, -75, 0, 10)
	openButton.BackgroundColor3 = getThemeColor()
	openButton.Text = "‚öôÔ∏è MENU"
	openButton.TextColor3 = Color3.fromRGB(255, 255, 255)
	openButton.TextSize = 18
	openButton.Font = Enum.Font.GothamBold
	openButton.BorderSizePixel = 0
	openButton.Parent = screenGui

	local openCorner = Instance.new("UICorner")
	openCorner.CornerRadius = UDim.new(0, 10)
	openCorner.Parent = openButton

	local openGradient = Instance.new("UIGradient")
	openGradient.Color = ColorSequence.new{
		ColorSequenceKeypoint.new(0, getThemeColor()),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(
			math.max(0, settings.themeR - 30),
			math.max(0, settings.themeG - 30),
			math.max(0, settings.themeB - 30)
		))
	}
	openGradient.Rotation = 45
	openGradient.Parent = openButton

	local openStroke = Instance.new("UIStroke")
	openStroke.Color = Color3.fromRGB(
		math.min(255, settings.themeR + 30),
		math.min(255, settings.themeG + 30),
		math.min(255, settings.themeB + 30)
	)
	openStroke.Thickness = 2
	openStroke.Parent = openButton

	openButton.MouseButton1Click:Connect(function()
		mainFrame.Visible = not mainFrame.Visible
	end)

	openButton.MouseEnter:Connect(function()
		openButton.BackgroundColor3 = Color3.fromRGB(
			math.min(255, settings.themeR + 20),
			math.min(255, settings.themeG + 20),
			math.min(255, settings.themeB + 20)
		)
	end)

	openButton.MouseLeave:Connect(function()
		openButton.BackgroundColor3 = getThemeColor()
	end)
end

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
loadConfig() -- –ó–∞–≥—Ä—É–∑–∏—Ç—å –∫–æ–Ω—Ñ–∏–≥ –ø—Ä–∏ —Å—Ç–∞—Ä—Ç–µ
sendNotification("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!", player.Name, 5)
playSound(SOUNDS.startup)
addLog("–°–∫—Ä–∏–ø—Ç —É—Å–ø–µ—à–Ω–æ –∑–∞–ø—É—â–µ–Ω")

createGUI()

-- Player ESP –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
for _, plr in pairs(Players:GetPlayers()) do
	if plr ~= player then
		createPlayerESP(plr)
	end
end

Players.PlayerAdded:Connect(function(plr)
	createPlayerESP(plr)
end)

Players.PlayerRemoving:Connect(function(plr)
	removePlayerESP(plr)
end)

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏—Ö –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–æ–≤
for _, obj in ipairs(visualsFolder:GetDescendants()) do
	if obj:IsA("Tool") then
		task.spawn(function()
			createESP(obj, true)
		end)
	end
end

visualsFolder.DescendantAdded:Connect(function(obj)
	if obj:IsA("Tool") then
		task.spawn(function()
			createESP(obj, true)
		end)
	end
end)

visualsFolder.DescendantRemoving:Connect(function(obj)
	if obj:IsA("Tool") then
		removeESP(obj)
	end
end)
